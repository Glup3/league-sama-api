{
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "printWidth": 120,
    "arrowParens": "always",
    "importOrder": ["^@/(.*)$", "^[./]"],
    "importOrderSeparation": true,
    "trailingComma": "all",
    "overrides": [
        {
            "files": ["*.json", ".prettierrc"],
            "options": {
                "tabWidth": 4
            }
        }
    ]
}
